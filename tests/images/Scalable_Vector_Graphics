<span class="nt">&lt;rect</span> <span class="na">fill=</span><span class="s">&quot;#fff&quot;</span> <span class="na">stroke=</span><span class="s">&quot;#000&quot;</span> <span class="na">x=</span><span class="s">&quot;-70&quot;</span> <span class="na">y=</span><span class="s">&quot;-70&quot;</span> <span class="na">width=</span><span class="s">&quot;390&quot;</span> <span class="na">height=</span><span class="s">&quot;390&quot;</span><span class="nt">/&gt;</span>
